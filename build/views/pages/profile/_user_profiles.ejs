<% let user_accs = [] %>
<% for (key in user) { %>
<% if (key === "local" && user[key].email !== undefined) { %>
<% user_accs.push({ key: key, value: user[key] }) %>
<% } %>
<% if (key === "google" && user[key].id !== undefined) { %>
<% user_accs.push({ key: key, value: user[key] }) %>
<% } %>
<% if (key === "facebook" && user[key].id !== undefined) { %>
<% user_accs.push({ key: key, value: user[key] }) %>
<% } %>
<% continue %>
<% } %>
<% if (user_accs.length === 1) { %>
<div class="col-sm-12">
    <% if (user_accs[0].key === "local") { %>
    <% include ./accounts/_local %>
    <% } %>
    <% if (user_accs[0].key === "google") { %>
    <% include ./accounts/_google %>
    <% } %>
    <% if (user_accs[0].key === "facebook") { %>
    <% include ./accounts/_facebook %>
    <% } %>
</div>
<% } else { %>
<% for (acc in user_accs) { %>

<div class="col-sm-6">
    <% if (user_accs[acc].key === "local") { %>
    <% include ./accounts/_local %>
    <% } %>
    <% if (user_accs[acc].key === "google") { %>
    <% include ./accounts/_google %>
    <% } %>
    <% if (user_accs[acc].key === "facebook") { %>
    <% include ./accounts/_facebook %>
    <% } %>
</div>
<% } %>
<% } %>





