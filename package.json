{
  "name": "ts_restaurant_finder_api",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/tests/*.test.ts",
    "start": "node build/app.js",
    "dev:start": "node build-dev/app.js",
    "dev": "npm run clean-build-ts-dev && npm run dev:start",
    "build": "npm run clean-build-ts",
    "build-ts": "tsc",
    "clean-build-ts": "rm -rf build/ && tsc && cp -rv src/server/views src/server/public build/ && cp -rv src/server/data build/data",
    "clean-build-ts-dev": "rm -rf build-dev/ && tsc --outDir build-dev && cp -rv src/server/data build-dev/data",
    "watch-ts": "tsc -w",
    "clean-all": "rm -rf build/ && rm -rf build-dev/"
  },
  "author": "Jaehyuk Rhee",
  "license": "ISC",
